{
  "manifest_version": 2,

  "name": "WebSphinx",
  "description": "__MSG_extDescription__",
  "version": "0.1",
  "default_locale": "en",

  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      }
    }
  },
  "permissions": [
    "activeTab",
    "nativeMessaging"
  ]
}
